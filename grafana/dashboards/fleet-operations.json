          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  d.friendly_name AS \"Printer\",\r\n  ps.state_text AS \"Status\",\r\n  ed.power_watts AS \"Live Power (W)\",\r\n  -- This is the crucial fix:\r\n  CASE\r\n    WHEN ps.is_printing = true THEN ps.progress_percent\r\n    ELSE 0\r\n  END AS \"Job Progress (%)\",\r\n  ps.filename AS \"Current Job\"\r\nFROM\r\n  devices AS d\r\nLEFT JOIN LATERAL (\r\n  SELECT * FROM printer_status WHERE device_id = d.device_id ORDER BY timestamp DESC LIMIT 1\r\n) ps ON true\r\nLEFT JOIN LATERAL (\r\n  SELECT * FROM energy_data WHERE device_id = d.device_id ORDER BY timestamp DESC LIMIT 1\r\n) ed ON true\r\nWHERE\r\n  d.location = '$factory' AND d.device_id IN ($device_id)\r\nORDER BY\r\n  d.friendly_name;",
          "refId": "A",
